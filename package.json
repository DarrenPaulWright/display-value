{
  "name": "display-value",
  "version": "1.3.1",
  "description": "Converts values to a readable display string",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "test-watch": "mocha --watch",
    "version": "npm run docs && git add docs",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "preversion": "npm test && npm run docs && git add --all && git diff --staged --quiet || git commit -m \"Updating docs\"",
    "postpublish": "git push --follow-tags",
    "deps": "ncu -u && npm install",
    "deps-commit": "npm run deps && git add --all && git diff --staged --quiet || git commit -m \"Updating dependencies\"",
    "deps-patch": "npm run deps-commit && npm run patch",
    "docs": "jsdoc2md index.js > README.md -t docs/docs.hbs --plugin dmd-readable"
  },
  "badges": [
    {
      "name": "npm",
      "image": "https://img.shields.io/npm/v/display-value.svg",
      "url": "https://npmjs.com/package/display-value"
    },
    {
      "name": "build",
      "image": "https://travis-ci.org/DarrenPaulWright/display-value.svg?branch=master",
      "url": "https://travis-ci.org/DarrenPaulWright/display-value"
    },
    {
      "name": "deps",
      "image": "https://david-dm.org/darrenpaulwright/display-value.svg",
      "url": "https://david-dm.org/darrenpaulwright/display-value"
    },
    {
      "name": "size",
      "image": "https://packagephobia.now.sh/badge?p=display-value",
      "url": "https://packagephobia.now.sh/result?p=display-value"
    },
    {
      "name": "vulnerabilities",
      "image": "https://snyk.io/test/github/DarrenPaulWright/display-value/badge.svg?targetFile=package.json",
      "url": "https://snyk.io/test/github/DarrenPaulWright/display-value?targetFile=package.json"
    },
    {
      "name": "license",
      "image": "https://img.shields.io/github/license/DarrenPaulWright/display-value.svg",
      "url": "https://npmjs.com/package/display-value/LICENSE.md"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DarrenPaulWright/display-value.git"
  },
  "keywords": [
    "display",
